---
"extends":
  - "eslint-config-defaults/configurations/walmart/es6-react"

"rules":
  "indent": [2, 4, {"SwitchCase": 1}]
  "max-len": 0
  "quotes": [2, "single", "avoid-escape"]
  "key-spacing": 0
  "no-use-before-define": 0
  "no-console": 0
  "react/jsx-indent-props": [2, 4]
  "react/prop-types": 0
"env":
  "browser": true,
  "node": true
